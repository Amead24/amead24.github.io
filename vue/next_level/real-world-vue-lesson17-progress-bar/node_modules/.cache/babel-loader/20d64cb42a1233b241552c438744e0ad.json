    next();\n    });\n  },\n  computed: mapState({\n    event: function event(state) {\n      return state.event.event;\n    }\n  })\n};",{"version":3,"sources":["EventShow.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;AA2BA,SAAA,QAAA,QAAA,MAAA;AACA,OAAA,SAAA,MAAA,WAAA;AACA,OAAA,KAAA,MAAA,eAAA;AAEA,eAAA;AACA,SAAA,CAAA,IAAA,CADA;AAEA,kBAFA,4BAEA,OAFA,EAEA,SAFA,EAEA,IAFA,EAEA;AACA,cAAA,KAAA,GADA,CAGA;AACA;AACA;AACA;AACA;;AACA,UAAA,QAAA,CAAA,kBAAA,EAAA,QAAA,MAAA,CAAA,EAAA,EAAA,IAAA,CAAA,YAAA;AACA;AACA;AACA;AACA;AACA,gBAAA,IAAA;AACA;AACA,KAPA;AAQA,GAlBA;AAmBA,YAAA,SAAA;AACA,WAAA;AAAA,aAAA,MAAA,KAAA,CAAA,KAAA;AAAA;AADA,GAAA;AAnBA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"event-header\">\n      <span class=\"eyebrow\">@{{ event.time }} on {{ event.date }}</span>\n      <h1 class=\"title\">{{ event.title }}</h1>\n      <h5>Organized by {{ event.organizer ? event.organizer.name : '' }}</h5>\n      <h5>Category: {{ event.category }}</h5>\n    </div>\n\n    <BaseIcon name=\"map\"><h2>Location</h2></BaseIcon>\n\n    <address>{{ event.location }}</address>\n\n    <h2>Event details</h2>\n    <p>{{ event.description }}</p>\n\n    <h2>Attendees\n      <span class=\"badge -fill-gradient\">{{ event.attendees ? event.attendees.length : 0 }}</span>\n    </h2>\n    <ul class=\"list-group\">\n      <li v-for=\"(attendee, index) in event.attendees\" :key=\"index\" class=\"list-item\">\n        <b>{{ attendee.name }}</b>\n      </li>\n    </ul>\n  </div>\n</template>\n<script>\nimport { mapState } from 'vuex'\nimport NProgress from 'nprogress'\nimport store from '@/store/store'\n\nexport default {\n  props: ['id'],\n  beforeRouteEnter(routeTo, routeFrom, next) {\n    NProgress.start()\n\n    // beforeRouteEnter doesn't have access to \"this\" to dispatch the\n    // action of the \"event\" store module, which uses the mutation to update\n    // the state with the new event object thus allowing this componenet to use it\n    // ----------------\n    // Instead the component needs to load in the state object and use its dispatch method\n    store.dispatch('event/fetchEvent', routeTo.params.id).then(() => {\n      // Assuming dispatch returns a promise ....\n      // \n      // once the api call is done (.then()) we can turn off the progress bar\n      // and using next() push the user onto their desired destination\n      NProgress.done()\n      next()\n    })\n  },\n  computed: mapState({\n    event: state => state.event.event\n  })\n}\n</script>\n<style scoped>\n.location {\n  margin-bottom: 0;\n}\n.location > .icon {\n  margin-left: 10px;\n}\n.event-header > .title {\n  margin: 0;\n}\n.list-group {\n  margin: 0;\n  padding: 0;\n  list-style: none;\n}\n.list-group > .list-item {\n  padding: 1em 0;\n  border-bottom: solid 1px #e5e5e5;\n}\n</style>\n"],"sourceRoot":"src/views"}]}